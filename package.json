{
  "name": "book-store-manager",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint:check": "eslint --ignore-path .gitignore --ext .js,.ts .",
    "lint:fixed": "eslint . --fix",
    "prettier:check": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "prettier:fixed": "prettier --write .",
    "lint_prettier:check": "yarn lint:check && yarn prettier:check"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.5",
    "autoprefixer": "^10.4.14",
    "daisyui": "^3.2.1",
    "postcss": "^8.4.26",
    "prettier": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.1",
    "react-router-dom": "^6.14.1",
    "redux": "^4.2.1",
    "tailwindcss": "^3.3.3"
  },
  "devDependencies": {
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "@vitejs/plugin-react": "^4.0.1",
    "eslint": "^8.44.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.1",
    "husky": ">=6",
    "lint-staged": ">=10",
    "typescript": "^5.0.2",
    "vite": "^4.4.0"
  },
  "lint-staged": {
    "*.{js,css,ts,tsx,jsx}": "yarn lint_prettier:check"
  }
}